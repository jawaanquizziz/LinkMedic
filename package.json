{
  "name": "linkmedic",
  "displayName": "LinkMedic",
  "description": "Finds broken image and file links in HTML, PHP as well as in MERN stack.",
  "publisher": "JawaanSanth",
  "icon": "Linkmedicicon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/jawaanquizziz/LinkMedic.git"
  },
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "mern",
    "react",
    "path-validator",
    "broken-links",
    "javascript",
    "typescript"
  ],
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:php",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "extensionKind": [
    "workspace",
    "ui"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "linkmedic.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "linkmedic.createFile",
        "title": "Linkmedic: Create Missing file"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}