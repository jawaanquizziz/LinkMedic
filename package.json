{
  "name": "linkmedic",
  "displayName": "LinkMedic",
  "description": "Finds broken image and file links in HTML and PHP.",
  "publisher": "JawaanSanth",
  "icon": "Linkmedicicon.png",
  "repository": {},
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onLanguage:html",
    "onLanguage:php"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "linkmedic.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
